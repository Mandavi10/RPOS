<div class="container pr-0" id="masterhome">
    <div class="pageHead">
        <h4 class="pageTitle">Welcome</h4>
        <div class="menuList" style="display:none" id="hmenulist">
            <a href="/Master/NewItem"><img src="/Content/images/add-icon.png">Add New Item</a>
            <a href="#" id="btnaddresturant" data-toggle="modal"><img src="/Content/images/restaurant-icon.png">Add Restaurant</a>
