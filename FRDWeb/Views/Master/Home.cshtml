                <h4 class="card-title">Select The Restaurant</h4>
            </div>
            <h4 class="card-title"></h4>
        </div>
        <div class="card-body mainCon" id="home">
        </div>
    </div>
    <div class="clearfix"></div>
</div>  


<div class="modal fade" id="addRestroPopup">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    <span class="cross">&times;</span>
                </button>
                <h4 class="modal-title">Add Restaurant</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12 col-md-12 p-0">
                        <div class="panel-group">
                            <div class="panel panel-default">

                                <div id="employee12" class="panel-collapse collapse show">
                                    <div class="panel-body">
                                     
                                          
                                                <div class="row">
                                                    <label class="col-sm-4 col-md-4 col-4 col-form-label">Restaurant Code:</label>
                                                    <div class="col-sm-8 col-md-8 col-8 pl-0">
                                                        <input type="text" class="form-control" placeholder="Restaurant Code" id="textcode" onkeypress="Removeclass();" maxlength="4" />
                                                    </div>
                                                </div>
                                        <div class="row">
                                            <label class="col-sm-4 col-md-4 col-4 col-form-label">Restaurant Name:</label>
                                            <div class="col-sm-8 col-md-8 col-8 pl-0">
                                                <input type="text" class="form-control" placeholder="Restaurant Name" id="textresturantname" onkeypress="Removeclass();" maxlength="15" />
                                            </div>
                                        </div>
                                        <div class="row">
                                            <label class="col-sm-4 col-md-4 col-4 col-form-label">Location:</label>
                                            <div class="col-sm-8 col-md-8 col-8 pl-0">
                                                <input type="text" class="form-control" placeholder="Location" id="textlocation" onkeypress="Removeclass();" maxlength="15" />
                                            </div>
                                        </div>
                                        <div class="row mb-2">
                                            <label class="col-sm-4 col-md-4 col-4 col-form-label">Address:</label>
                                            <div class="col-sm-8 col-md-8 col-8 pl-0">
                                                <textarea type="text" class="form-control" placeholder="Address" id="textaddress" onkeypress="Removeclass();"></textarea>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <label class="col-sm-4 col-md-4 col-4 col-form-label">Arabic Name:</label>
                                            <div class="col-sm-8 col-md-8 col-8 pl-0">
                                                <input type="text" class="form-control" placeholder="Arabic Name" id="textarabicname" onkeypress="Removeclass();"  maxlength="15"/>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <label class="col-sm-4 col-md-4 col-4 col-form-label">VAT No.:</label>
                                            <div class="col-sm-8 col-md-8 col-8 pl-0">
                                                <input type="text" class="form-control" placeholder="VAT No" id="textvatno" onkeypress="Removeclass();"  maxlength="16"/>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <label class="col-sm-4 col-md-4 col-4 col-form-label" id="lblupload">Browse Image:</label>
                                            <div class="col-sm-8 col-md-8 col-8 pl-0">
                                                <input type="file" id="files" name="pic" accept="image/*">
                                            </div>
                                        </div>
                                          
                                      
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer pr-2">
                @*<button type="button" class="btn btn-success mr-1" id="btnDelete">Delete</button>
                <button type="button" class="btn btn-success mr-1" id="btnUpdate">Update</button>*@
                <button type="button" class="btn btn-success " id="btnsave">Save</button>
                

            </div>
        </div>

    </div>
</div>